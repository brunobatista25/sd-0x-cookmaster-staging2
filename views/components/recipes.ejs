<section class="box box90 flex-row">
  <% recipes.forEach((recipe) => {%>
  <section class="box box30 box-border flex-column">
    <h2><%= recipe.recipeName %></h2>
    <h3><%= recipe.userName.toLowerCase(); %></h3>
    <% if(recipe.isNull) { %>
    <p>Nenhuma receita encontrada para esta busca</p>
    <% } else {%>
    <a onclick="window.location.pathname='<%= `/recipes/${recipe.recipeId}` %>'">
      Ver mais
    </a>
    <%} %>
  </section>
  <% }) %>
</section>
